		/* parte clara - banners */
		#claro #hWrapper{
			width: 100%;
			height: 58px;
			margin: 0;
			padding: 0;
		}
			#hWrapper #seta_esquerda {
				float: left;
				vertical-align: top;
				height: 58px;
				width: 26px;
			}
			#hWrapper #seta_direita{
				float: right;
				vertical-align: top;
				height: 58px;
				width: 26px;
			}

			#hWrapper .scrollable {
				float: left;
				height: 58px;
				overflow: hidden;
				position: relative;
				vertical-align: top;
				width: 938px;
			}

				.scrollable .items {
					clear: both;
					height: 58px;
					position: absolute;
					width: 20000em;
				}
				
				.scrollable .items div {
					float: left;
					height: 58px;
					width: 938px;
				}
	
				.scrollable .items div a.banner {
					display: inline-block;
					width: 121px;
					height: 58px;
					margin: 0;
					margin-right: 10px;
				}

					.scrollable .items div a img {
						display: block;
						width: 119px;
						height: 56px;
						border: 1px solid #fff;
					}

					.scrollable .items div a:hover img {
						border: 1px solid #e0b35e;
					}
	
				.scrollable .items div .banner:first-of-type {
					margin-left: 6px;
				}
	
				.scrollable .items div .banner:last-of-type {
					margin-right: 0;
				}

