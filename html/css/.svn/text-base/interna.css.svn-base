	@charset "utf-8";
	strong{
		font-size: 10pt;
	}

	#conteudo .titulo{
		font-size: 10pt;
		font-weight: bold;
		color: #661c42;
		width: 765px;
		height: 24px;
		border-bottom: solid #661c42 1px;
		margin-bottom: 5px;
		display: inline-block;
	}

	.titulo .acessibilidade2 {
		
		background: none repeat scroll 0 0 #e9e9e9;
		float: right;
		padding: 0 0 3px 8px;
	}
	.titulo .acessibilidade2 img{
		margin-left: 8px;
		margin-top: 10px;
	}

	#destaque.scrollable {
		
		height: 493px;
		position:relative;
		overflow:hidden;
		width:140px;
	}
	#destaque.scrollable .items{
		width: 138px;
		height:20000em;
		position:absolute;
		

	}
	#destaque.scrollable .items div {
		float:left;
		width:140px;
		height: 493px;
	}
	#conteudo .coluna_direita {
		
		background: #ffffff;
		/* Old browsers */

		background: -moz-linear-gradient(top, #e9e9e9 0%, #ffffff 100%);
		/* FF3.6+ */

		background: -webkit-gradient(linear, left top, left bottom, color-stop(0%, #e9e9e9), color-stop(100%, #ffffff));
		/* Chrome,Safari4+ */

		background: -webkit-linear-gradient(top, #e9e9e9 0%, #ffffff 100%);
		/* Chrome10+,Safari5.1+ */

		background: -o-linear-gradient(top, #e9e9e9 0%, #ffffff 100%);
		/* Opera 11.10+ */

		background: -ms-linear-gradient(top, #e9e9e9 0%, #ffffff 100%);
		/* IE10+ */

		background: linear-gradient(to bottom, #e9e9e9 0%, #ffffff 100%);
		/* W3C */

		filter: progid:dximagetransform.microsoft.gradient(startColorstr=#e9e9e9, endColorstr=#ffffff, GradientType=0);
		/* IE6-9 */
		width: 140px;
		min-height: 493px;
		vertical-align: top;
		display: inline-block;
		padding: 20px;
		margin: -30px 0px 0 40px;

	}
	.coluna_direita #seta_esquerda{
		margin: 0 auto;
		height:  26px;
		width:98px;

	}
	.coluna_direita #seta_direita{
		margin: 0 auto;
		height:  26px;
		width:98px;
	}

	.items a img {
		border: 1px solid #FFF;
		height: 98px;
		width: 137px;

	}
	.items a:hover img {
		border: 1px solid #e0b35e;

	}
	.items div span#foto {
		display: inline-block;
		*float: left;
		width: 137px;
		height: 98px;
		margin: 20px 1px 1px;
		*margin: 0 1px 6px 5px;

	}
	/* formularios */
	form label {
		display: block;
		line-height:25px;
		font-weight: bold;
	}
	 #form_contato {
		display: block;
		margin: 15px auto 30px;
		width: 414px;
		height: auto;
	}
	#form_contato .area_campo{
		display: block;
		height: auto;
		margin: 0 auto;
		vertical-align: top;
		width: 435px;
	}
	form .area_campo input {
		vertical-align: middle;
		height:20px;
		width:405px;
		border:solid #e0b35e 1px;
		padding:3px;
		font-family: "Arial",Tahoma,Verdana,Helvetica,sans-serif;
		font-size: 8pt;
		color:#888;
		/* bordas arredondadas */
		-o-border-radius: 8px;
		-khtml-border-radius: 8px;
		-moz-border-radius: 8px;
		-webkit-border-radius: 8px;
		border-radius: 8px;
	}

	form select {
		float: right;
		height:20px;
		width:405px;
		border:solid #DDDBD9 1px;
		margin: 5px 0 0 0;
		padding:3px;
		font-family: "Arial",Tahoma,Verdana,Helvetica,sans-serif;
		font-size: 8pt;
		color:#888;

		/* bordas arredondadas */
		-o-border-radius: 8px;
		-khtml-border-radius: 8px;
		-moz-border-radius: 8px;
		-webkit-border-radius: 8px;
		border-radius: 8px;
	}

	form .area_campo_maior textarea {

		border:solid #e0b35e 1px;
		height:140px;
		width:400px;
		max-height:589px;
		max-width:589px;
		margin:5px 0 5px 0;
		padding:5px;

		font-family: "Arial",Tahoma,Verdana,Helvetica,sans-serif;
		font-size: 8pt;
		color:#888;

		/* bordas arredondadas */
		-o-border-radius: 8px;
		-khtml-border-radius: 8px;
		-moz-border-radius: 8px;
		-webkit-border-radius: 8px;
		border-radius: 8px;
	}

	form input:hover, form select:hover, form textarea:hover {
		border-color: #922354;
	}

	form input:focus, form select:focus, form textarea:focus {
		border-color: #922354;
	}


	form .area_botoes button {
		float:right;
		height: 36px;
		width: 60px;
		cursor:pointer;
		margin-left: 5px;

		font-family: "Arial",Tahoma,Verdana,Helvetica,sans-serif;
		font-size: 9pt;
		color: #FFF;

		background: #e0b35e;
		/* bordas arredondadas */
		-o-border-radius: 8px;
		-khtml-border-radius: 8px;
		-moz-border-radius: 8px;
		-webkit-border-radius: 8px;
		border-radius: 8px;

		border: none;
	}

	/* bloco-padrão das internas */
	#conteudo #contato, #conteudo #interna_noticias, #conteudo #pagina {
		width: 765px;
		padding-top: 20px;
		display: inline-block;
		vertical-align: top;
	}
	#conteudo .formulario{
		width: 414px;
		display: inline-block;
			
	}
	/*----NOTÍCIAS----*/
	#conteudo #interna_noticias ul{
		margin: 0px 30px 0 ;
	}
	#conteudo #interna_noticias ul li {
		background: transparent url(../img/marcador.png) no-repeat scroll 1px 9px !important;
		line-height: 1.9em;
		 padding-left: 0;
	}
	#conteudo #interna_noticias ul li a {
		margin-left: 15px;
		color: #4a4a4a;
		line-height: 1.9em;
		font-size: 10pt;
	}
		#conteudo #interna_noticias ul li a:hover {
		color: #922354;

	}

	/*páginas*/
	#conteudo #pagina .data-noticia{
		float: left;
		position: absolute;
		font-style: italic;
		font-size: smaller;
		text-indent: 0;
		color: #ae8f94;
		z-index: 10;
		margin: -18px 0 0;
		position: relative;
	}

	#conteudo  a{
		color: #661c42;
	}
	#conteudo  a:hover{
		color: #ae8f94;
	}
	#conteudo #pagina img{
		float: left;
		padding: 3px;
		border: solid 1px #e0b35e;
		width: 192px;
		height: 133px;
		background: #ffffff;
		margin-right: 10px;
		margin-left: 10px;
		margin-top: 5px;
		margin-bottom: 5px;
		display: inline-block;

	}
	#conteudo #pagina .img-pagina{
		height: 180px;
		width: 220px;
		float: left;
		display: inline-block;

	}
	#conteudo #pagina .img-pagina .legenda-foto{
		color: #ae8f94;
		font-style: italic;
		font-size: smaller;
		text-indent: 0;
		margin: 35px;
	}
	#conteudo li{
		display:block;
		height:auto;
		width:auto;
		background: transparent url(../img/marcador.png) no-repeat scroll 1px 14px !important;
		font-size:10pt;
		line-height:18px;
		text-align:justify;
		margin:0 0 5px 0;
		padding-left:20px;
	}
	#conteudo p{
		font-size:10pt;
		line-height:18px;
		padding-bottom:20px;
		text-align:justify;
	}
	#conteudo #pagina .rodape-interna{
		border-top: solid 1px #661c42;
		padding-top: 5px;
		
		
	}
	#conteudo #pagina .rodape-interna a {
		color: #e0b35e;

	}
	#conteudo #pagina .rodape-interna a:hover {
		color: #ae8f94;
	}
	#conteudo #pagina .rodape-interna img{
		vertical-align: middle;
		margin-right: 5px;
		height: auto;
		margin-right: 5px;
		vertical-align: middle;
		width: auto;
		border: none;
		background: transparent;
		float: none;
		margin-left: 0;

	}


