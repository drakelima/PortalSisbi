	min-width: 990px;
	height: 165px;
}

header section#centro, footer section#centro {
	display: block;
	width: 990px;
	height: auto;
	margin: 0px auto;
	padding-left: 5px;
	padding-right: 5px;
}
/*-----MENU-----*/
#centro #nav{
	display:block;

	height:30px;
	width:100%;
}

#nav #menu{

	height:100%;
	width:990px;
	margin:0 auto;					
}

#menu a{
	color:#661c42;
}
#menu a:hover{
	text-decoration:none;
	color: #cfbfc2;
}
#nav #menu #menu-esquerdo{
	margin: -85px 0px 0px 0px;
}
#nav #menu #menu-esquerdo ul li{
	float:left;
	color:#FFF;
	margin: 0px 45px 0 0;
}

#menu #menu-direito ul li{
	float:right;
	color:#FFF;
	margin: 0px 0 0 54px;
}

/* logo */
#centro #logo {
	/*display: inline-block;*/
	width:320px;
					/*height: 164px;
					width: 50%;*/
					margin: 0px auto;
				}

				#logo img.imglogo {

					display: block;
					background-image: url(../img/logo.png);
				}

				/*----conteudo----*/
				#bg section#conteudo {
					display: block;
					width: 990px;
					min-height: 435px;
					height: auto;
					margin: 0 auto;
					background: #e9e9e9;

					z-index: -2;
					padding-top: 30px;
					padding-bottom:15px;

				}
				/**nivo slider**/
				#area_animacao .nivoSlider {
					position:relative;
					width:100%;
					height:auto;
					overflow: hidden;
				}
				#area_animacao .nivoSlider img {
					position:absolute;
					top:0px;
					left:0px;
					max-width: none;
				}

				.nivo-main-image {
					display: block !important;
					position: relative !important; 
					width: 100% !important;
					overflow: hidden
				}

				/* If an image is wrapped in a link */
				.nivoSlider a.nivo-imageLink {
					position:absolute;
					top:0px;
					left:0px;
					width:100%;
					height:100%;
					border:0;
					padding:0;
					margin:0;
					z-index:6;
					display:none;
					background:white; 
					filter:alpha(opacity=0); 
					opacity:0;
				}
				/* The slices and boxes in the Slider */
				.nivo-slice {
					display:block;
					position:absolute;
					z-index:5;
					height:100%;
					top:0;
					min-height: 245px;
					min-width: 480px;
				}
				.nivo-slice img{
					min-height: 245px;
					min-width: 480px;
				}
				.nivo-box {
					display:block;
					position:absolute;
					z-index:5;
					overflow:hidden;
				}
				.nivo-box img { display:block; }

				/* Caption styles */
				#slider .nivo-caption {
					position:absolute;
					left:0px;
					bottom:0px;
					background:#000;
					color:#e0b35e;
					width:100%;
					height: 30px;
					z-index:8;
					padding: 5px 10px;
					opacity: 0.8;
					overflow: hidden;
					display: none;

				}
				.nivo-caption p {
					padding:5px;
					margin:0;
				}
				.nivo-caption a {
					display:inline !important;
				}
				.nivo-html-caption {
					display:none;
				}

				.nivo-controlNav {
					background: none repeat scroll 0 0 #661C42;
					margin: -40px 0 1px 391px;
					position: absolute;
					z-index: 999;
					padding: 10px;
					height: 20px;
				}
				.nivo-controlNav a {
					cursor:pointer;
					margin: 3px;
					color: #cfbfc2;
				}
				.nivo-controlNav a.active {
					color: #ffffff;
				}

				/**fim nivo slider**/

				#area_animacao img{
					width: 480px;
					height: 245px;
					max-width: 480px;
					max-height: 245px;
				}
				.cs-buttons { font-size: 0px;  float: left; }
				.cs-buttons a { margin-left: 5px; height: 10px; width: 10px; float: left; border: 1px solid #B8C4CF; color: #B8C4CF; text-indent: -1000px; }
				.cs-active { background-color: #B8C4CF; color: #FFFFFF; }

				.cs-title { width: 480px; height: 50px; background-color: #000000; color: #e0b35e; font-size: 18px; }


				#conteudo #area_animacao {
					background: #fff;
					border: 1px solid #661c42;
					display: inline-block;
					height: 245px;
					width: 480px;
					max-height: 245px;
					max-width: 480px;
					vertical-align: top;
				}
				#area_animacao .animacao {
					display: block;
					width: 622px;
					height: 181px;
					margin: 0 0 30px;
					padding: 11px 0 0;
					background: #dadff1;
					margin-left: 90px;

				}

				/*----noticias----*/
				#conteudo #noticias{
					background: #fff;
					border: 1px solid #661c42;
					display: inline-block;
					height: 215px;
					width: 450px;
					vertical-align:top;
					float: right;
					padding:15px;
				}
				#noticias strong{
					color: #661c42;
					font-size:18px;
				}
				/*----- noticias destaque -----*/
				#noticias #list-noticia{
					display:block;
					padding-left: 5px;
					height: 163px;
				}
				#list-noticia ul {
					list-style: inside square;
					width: auto;

				}
				#list-noticia ul li {
					color:#e0b35e;
					line-height: 1.9em;
				}
				#list-noticia ul li a{
					display: inline;
					width: 100%;
					height: auto;
					text-decoration: none;
					color: #4a4a4a;
					cursor: pointer;
				}
				#list-noticia ul li a:hover{
					color: #7A204B;
				}
				#list-noticia ul li a label.data{
					font-weight: bold;
					cursor: pointer;
				}
				#noticias .mais{
					width: 86px;
					height: 20px;
					padding: 5px;
					color: #ffffff;
					background: #661c42;
					float: right;
					display: block;
				}
				/*------parceiros-----*/
