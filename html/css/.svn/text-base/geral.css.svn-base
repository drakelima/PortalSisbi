@charset "utf-8";

/*----estilo----*/
*, div, ul, ol, li, a, img, form, input, select, textarea, button {
	margin: 0;
	padding: 0;
	outline: 0 !important;
}

ul,ol {
	list-style:none;
}

li {
	vertical-align:top;
	line-height:1.2em;
}

li a, li span {
	line-height:1.2em
}

a {
	color: #ffffff;
	text-decoration: none;
	cursor: pointer;
}

a:hover {
	text-decoration: none;
}

img {
	border: none;
}

h1 {
	font-size: 13pt;
	font-weight: bold;
	text-transform: capitalize;
	margin-bottom: 15px;
	
}
label{
	font-size: 12px;
}

p {
	font-size: 9pt;
	text-align: left;
	line-height: 17pt;
}
/*----fim estilo----*/
/*----body----*/
body{
	display: block;
	font-family: "Arial",Tahoma,Verdana;
	color: #4a4a4a;
	background: #e9e9e9;
	font-size: 11pt;
}

#barra-ufrn {
	height: 46px;
	position: relative;
	overflow: hidden;
}
.barra{
	background-color: #6e5252; 
}
#barra-ufrn #barra-marca{
	width:990px;
	color: #fff;
	margin: 8px auto;
	display: block;
	height:29px;
}
#barra-marca img{
	display: inline-block;
	vertical-align: middle;
}
#barra-marca span{
	margin-left: 7px;
	vertical-align: middle;
	font-size: 14px;
	border-left: solid 1px #fff;
	padding-left: 5px;
}
#barra-marca .selecao{
	width: 235px;
	float: right;
	height: 29px;
	background: url(../img/select.png) no-repeat right #cfbfc2;
	overflow: hidden;
	border-radius: 2px;
}
.selecao select{
	
	background: transparent;
	width: 257px;
	padding: 5px;
	font-size: 12px;
	font-style: italic;
	color: #6e5252;
	border: 0;
	
	height: 29px;
	-webkit-appearance: none;
}
/***acessibilidade***/
#barra-ufrn #barra-marca .acessibilidade{
	display: inline-block;
	width: 50px;
	float: right;
	margin-right: 10px;
	height: 29px;
	margin-top: 5px;
}
